// FFI defintions
/@ method ffiprint_int(a: Int, b: Int, c: Int, d: Int) @/

fun main() {
    return 0;
}

fun foo(1 x) {
    /@ ensures x == retval @/
    return x;
}

fun call() {
    foo(2);
    return 0;
}

fun tail_call() {
    // todo: follow up this with Halogen
    var y = foo(3);
    return y;
}

fun ext_call() {
    @print_int(0,0,0,1);
    return 0;
}

fun dec_call() {
    var 1 x = foo(2);
    x = x + 2;
    return x;
}
